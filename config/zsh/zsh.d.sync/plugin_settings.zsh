# zdharma-continuum/fast-syntax-highlighting
export ZSH_AUTOSUGGEST_HIGHLIGHT_STYLE='fg=243'
export ZSH_AUTOSUGGEST_STRATEGY='match_prev_cmd'

# lukechilds/zsh-nvm
export NVM_LAZY_LOAD=true
# export NVM_NO_USE=true
export NVM_LAZY_LOAD_EXTRA_COMMANDS=('vim', 'nvim', 'node', 'apollo')

# rtx
export RTX_PYTHON_DEFAULT_PACKAGES_FILE=$HOME/.config/rtx/default-python-packages
export RTX_NODEJS_DEFAULT_PACKAGES_FILE=$HOME/.config/rtx/default-nodejs-packages
