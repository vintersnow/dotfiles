{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      0,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      1,
      "always"
    ],
    "strict": [2, "global"],
    "no-console":0
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ]
}
